<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
      <item  >
           <shape  >
                <stroke  android:color="#FF0000FF" android:width="1dip"/>
                <solid   android:color="#b0000000"/>
                <padding  android:left="3dip" android:top="3dip" android:right="3dip" android:bottom="3dip"/>
                <corners   android:topLeftRadius="3dip" android:topRightRadius="3dip" android:bottomLeftRadius="4dip" android:bottomRightRadius="4dip"/>
           </shape>
      </item>
   
       <item   android:state_selected="true">
           <shape  >
                <stroke  android:color="#FF0000FF" android:width="1dip"/>
                <solid   android:color="#808000"/>
                <padding  android:left="3dip" android:top="3dip" android:right="3dip" android:bottom="3dip"/>
                <corners   android:topLeftRadius="3dip" android:topRightRadius="3dip" android:bottomLeftRadius="4dip" android:bottomRightRadius="4dip"/>
           </shape>
      </item>
</selector>
